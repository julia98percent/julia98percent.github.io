{
  "compilerOptions": {
    "target": "ES5",
    "lib": ["dom", "esnext"],
    "jsx": "react",
    "module": "CommonJS",
    "checkJs": true,
    "sourceMap": true,
    "noEmitOnError": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "allowUnusedLabels": true,
    "allowUnreachableCode": true,
    "esModuleInterop": true
  },
  "include": ["./src/**/*", "./gatsby-node.ts", "./gatsby-config.ts"],
  "exclude": ["node_modules"]
}
